<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush Color="White" x:Key="MainBackground"/>
    <SolidColorBrush Color="#dfe6e9" x:Key="SecondaryBackground"/>
    <SolidColorBrush Color="#fbfbfb" x:Key="CardBackground"/>
    <SolidColorBrush Color="#979197" x:Key="CardBorderForeground"/>
    <SolidColorBrush Color="#1c1b21" x:Key="CardMainForeground"/>
    <SolidColorBrush Color="#979197" x:Key="CardSecondaryForeground"/>

    <SolidColorBrush Color="#3f3244" x:Key="InvertedSecondaryBackground"/>
    <SolidColorBrush Color="#f5f5f5" x:Key="InvertedSecondaryForeground"/>

    <SolidColorBrush Color="#bfbfbf" x:Key="MainBackgroundLighter"/>
</ResourceDictionary>